{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.swcc.finance.SSP_Finance",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"ZSSP_USER_SRV": {
				"uri": "/sap/opu/odata/sap/ZSSP_USER_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"ZSSP_COMMON": {
				"uri": "/sap/opu/odata/sap/ZSSP_COMMON/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"ZAPI_BUSINESS_PARTNER": {
				"uri": "/sap/opu/odata/sap/API_BUSINESS_PARTNER/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.swcc.finance.SSP_Finance.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.swcc.finance.SSP_Finance.i18n.i18n"
				}
			},
			"ZSSP_USER_SRV": {
				"dataSource": "ZSSP_USER_SRV",
				"preload": true
			},
			"ZSSP_COMMON": {
				"dataSource": "ZSSP_COMMON",
				"preload": true
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": false
			}

		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.swcc.finance.SSP_Finance.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": [
						"notFound"
					]
				}
			},
			"routes": [{
					"name": "RouteApp",
					"pattern": "",
					"target": [
						"TargetApp"
					]
				}, {
					"name": "ModuleSelect",
					"pattern": "ModuleSelect",
					"target": [
						"ModuleSelect"
					]
				}, {
					"name": "CustRegistration",
					"pattern": "",
					"target": [
						"CustRegistration"
					]
				}, {
					"name": "HomePage",
					"pattern": "HomePage",
					"target": [
						"HomePage"
					]
				}, {
					"name": "SlaCreation",
					"pattern": "SlaCreation",
					"target": [
						"SlaCreation"
					]
				}, {
					"name": "FinanceRequest",
					"pattern": "FinanceRequest",
					"target": [
						"FinanceRequest"
					]
				}, {
					"name": "FinanceRequestView",
					"pattern": "FinanceRequestView",
					"target": [
						"FinanceRequestView"
					]
				}

			],
			"targets": {
				"TargetApp": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "CustomerRegistration"
				},
				"ModuleSelect": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "ModuleSelect"
				},
				"LandingView": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "ScmCreateRequest"
				},
				"CustRegistration": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "CustomerRegistration"
				},
				"HomePage": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "HomePage"
				},

				"SlaCreation": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "SlaCreation"
				},
				"FinanceRequest": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "FinanceCreateRequest"
				},

				"FinanceRequestView": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "FinanceViewRequest"
				}
			}
		}
	}
}